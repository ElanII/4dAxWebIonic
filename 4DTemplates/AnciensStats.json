<!--#4DCODE
if($1#"")

query([Ancien];[Ancien]Nom_usuel=$1+"@";*)
query([Ancien];|;[Ancien]prenom=$1+"@";*)
query([Ancien];|;[Ancien]promotion_etude=$1+"@";*)
query([Ancien];|;[Ancien]Code_nationalite=$1+"@";*)
query([Ancien];|;[Ancien]AdrC_pays=$1+"@";*)
